CREATE TABLE CUSTOMER (
	id			VARCHAR(20) 	PRIMARY KEY, 
	firstName 	VARCHAR(20), 
	lastName 	VARCHAR(20), 
	dob 		VARCHAR(20), 
	title 		VARCHAR(20)
);


CREATE TABLE ACCOUNT (
	accountNo	NUMERIC(10) 	PRIMARY KEY, 
	branchCode 	NUMERIC(5), 
	accountType	NUMERIC(1), 
	balance		NUMERIC(10, 2), 
	cust_id		VARCHAR(20),
	FOREIGN KEY (cust_id) REFERENCES CUSTOMER(ID)
);

CREATE TABLE BANK (
	bankCode	VARCHAR(4)		PRIMARY KEY,
	bankName	VARCHAR(20)
);

CREATE TABLE BRANCH (
	branchCode	NUMERIC(4)		PRIMARY KEY,
	bankCode 	VARCHAR(4),
	FOREIGN KEY (bankCode) REFERENCES BANK(bankCode)
);


ALTER TABLE CUSTOMER DROP COLUMN dob; 

ALTER TABLE CUSTOMER ADD COLUMN DOB DATE;

SELECT * FROM CUSTOMER;
SELECT * FROM BANK;
SELECT * FROM BRANCH;
SELECT * FROM ACCOUNT;
