select * from dept WHERE Location='MUM';

DELETE FROM EMP WHERE EMPID=1091;

SELECT DEPTID, AVG(SAL)
FROM emp
WHERE SAL > 5000
GROUP BY deptid;

SELECT e.ename, d.dname from emp e LEFT OUTER JOIN dept d on (d.deptid = e.deptid);
SELECT ename, dname from EMP JOIN dept USING (deptid);
SELECT ename, dname from EMP NATURAL JOIN dept;


SELECT name, population, countrycode from city where countrycode != 'IND' AND population > (SELECT population from city where name='Mumbai (Bombay)')