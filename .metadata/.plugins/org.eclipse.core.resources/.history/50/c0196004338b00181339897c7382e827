import java.util.Date;

public class LoanAccount extends Account {
	private int totalOutstanding;

	public LoanAccount(int accountNo, int customerId, int branchCode, Date openingDate, int totalOutstanding) {
		super(accountNo, customerId, branchCode, openingDate);
		this.totalOutstanding = totalOutstanding;
	}

	@Override
	public String toString() {
		return super.toString() + " LoanAccount [totalOutstanding=" + totalOutstanding + "]";
	}
	

}
